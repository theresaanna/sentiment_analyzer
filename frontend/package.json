{
  "name": "sentiment-analyzer-frontend",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && vite-bundle-visualizer",
    "preview": "vite preview --port 5173",
    "test": "vitest run --environment jsdom",
    "test:watch": "vitest --environment jsdom",
    "test:coverage": "vitest run --environment jsdom --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,json,css}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,json,css}'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ../app/static/react/*",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully!'"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sparklefall": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "terser": "^5.27.0",
    "typescript": "^5.4.5",
    "vite": "^5.4.8",
    "vite-bundle-visualizer": "^1.0.1",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
